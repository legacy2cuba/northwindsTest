<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window  xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.xformix.northwindstest.forms.customer_orders_subform2.Customer_orders_subform2Edit"
        datasource="orderdetailsextendedDs"
        focusComponent="fieldGroup"
        messagesPack="com.xformix.northwindstest.forms.customer_orders_subform2">
    <dialogMode height="600"
                width="800"/>
<layout spacing="true" >
<grid spacing="true" height="100%" width="100%"  >
  <columns count="1"/>
<rows>
<row flex="1.0">

      <groupTable id="Detail"  width="100%" height="100%" x="0" y="0" >
          <rows datasource="orderdetailsextendedDs"/>
          <actions>
              <action id="create"/>
              <action id="edit"/>
              <action id="remove"/>
          </actions>
          <columns>
              <column id="productname" caption="productname" property="productname"/>
              <column id="unitprice" caption="unitprice" property="unitprice"/>
              <column id="quantity" caption="quantity" property="quantity"/>
              <column id="discount" caption="discount" property="discount"/>
              <column id="extendedprice" caption="extendedprice" property="extendedprice"/>
          </columns>
      </groupTable>
</row>
</rows></grid>
</layout>
</window>
