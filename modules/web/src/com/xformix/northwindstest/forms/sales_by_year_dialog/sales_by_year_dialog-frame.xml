<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window  xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.xformix.northwindstest.forms.sales_by_year_dialog.Sales_by_year_dialogEdit"
        datasource="nodatasourceDs"
        focusComponent="fieldGroup"
        messagesPack="com.xformix.northwindstest.forms.sales_by_year_dialog">
    <dialogMode height="600"
                width="800"/>
<layout spacing="true" >
    <scrollBox width="100%" height="100%" spacing="false" margin="false">
    <vbox id="TOP_LEVEL" width="322.56px" height="100%" >
      <grid spacing="true" id="Detail"  width="100%"  >
        <columns count="2"/>
        <rows>
          <row>
              <label  id="BeginningDateLabel" value="Enter beginning date:"  height="20.0px" row="0" column="0"  align="MIDDLE_RIGHT" htmlEnabled="false" stylename="access-label"/>
            <textField  id="BeginningDate" height="20.0px" width="80.0px" row="0" column="1"  stylename="access-textfield"  align="MIDDLE_LEFT"/>
          </row>
          <row>
              <label  id="EndingDateLabel" value="Enter ending date:"  height="20.0px" row="1" column="0"  align="MIDDLE_RIGHT" htmlEnabled="false" stylename="access-label"/>
            <textField  id="EndingDate" height="20.0px" width="80.0px" row="1" column="1"  stylename="access-textfield"  align="MIDDLE_LEFT"/>
          </row>
          <row>
              <label  id="ShowDetailsLabel" value="Show details?"  height="20.0px" row="2" column="0"  align="MIDDLE_RIGHT" htmlEnabled="false" stylename="access-label"/>
            <checkBox  id="ShowDetails" height="20.0px" width="80.0px" row="2" column="1" />
          </row>
          <row>
            <button  id="OK" value="OK"  height="20.0px" width="100.0px" row="3" column="0"  caption="OK" invoke="doActionOK" stylename="access-button"/>
            <button  id="Cancel" value="Cancel"  height="20.0px" width="80.0px" row="3" column="1"  caption="Cancel" invoke="doActionCancel" stylename="access-button"/>
          </row>
        </rows>
      </grid>
</vbox>
</scrollBox>
</layout>
</window>
